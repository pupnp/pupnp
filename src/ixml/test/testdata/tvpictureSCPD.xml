  <serviceStateTable>

    <stateVariable sendEvents="yes">
      <name>Color</name>
      <dataType>i4</dataType>
        <allowedValueRange>
          <minimum>1</minimum>
          <maximum>10</maximum>
          <step>1</step>
        </allowedValueRange>
      <defaultValue>5</defaultValue>
    </stateVariable>

    <stateVariable sendEvents="yes">
      <name>Tint</name>
      <dataType>i4</dataType>
