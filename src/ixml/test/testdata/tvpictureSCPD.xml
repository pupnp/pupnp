    <action>
      <name>SetContrast</name>
      <argumentList>
        <argument>
        <name>NewContrast</name>
          <retval />
          <relatedStateVariable>Contrast</relatedStateVariable>
          <direction>out</direction>
        </argument>  
        <argument>
        <name>Contrast</name>
          <relatedStateVariable>Contrast</relatedStateVariable>
          <direction>in</direction>
        </argument>
      </argumentList>
